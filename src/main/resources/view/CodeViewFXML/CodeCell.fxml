<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.StackPane?>

<HBox fx:id="backgroundLayout" alignment="CENTER_LEFT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" onMouseClicked="#layoutOnEnter" prefHeight="50.0" spacing="10.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.CodeViewControllers.CodeCellController">
   <children>
      <StackPane fx:id="stackpane">
         <children>
            <Label fx:id="code" />
            <TextField fx:id="userInput" maxWidth="-Infinity" onKeyPressed="#TextFieldOnAction" />
         </children>
      </StackPane>
      <Region HBox.hgrow="ALWAYS" />
      <Button fx:id="copy" maxWidth="-Infinity" mnemonicParsing="false" onAction="#copyOnAction" text="Copy" HBox.hgrow="ALWAYS">
         <padding>
            <Insets bottom="8.5" left="14.0" right="14.0" top="8.0" />
         </padding></Button>
      <Button fx:id="delete" maxWidth="-Infinity" mnemonicParsing="false" onAction="#deleteOnAction" text="Delete" HBox.hgrow="ALWAYS">
         <padding>
            <Insets bottom="8.5" left="10.0" right="10.0" top="8.0" />
         </padding></Button>
      <Button fx:id="edit" maxWidth="-Infinity" mnemonicParsing="false" onAction="#editOnAction" text="Edit" HBox.hgrow="ALWAYS">
         <padding>
            <Insets bottom="8.5" left="18.0" right="18.5" top="8.0" />
         </padding></Button>
   </children>
   <padding>
      <Insets left="10.0" right="10.0" />
   </padding>
</HBox>
