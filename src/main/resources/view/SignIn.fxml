<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="background" alignment="CENTER" prefHeight="250.0" prefWidth="300.0" spacing="12.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.SignInController">
    <children>
          <Label fx:id="Title" alignment="CENTER" text="Welcome Back!" textAlignment="CENTER" wrapText="true" VBox.vgrow="ALWAYS">
         <VBox.margin>
            <Insets bottom="4.5" />
         </VBox.margin></Label>
        <HBox fx:id="usernameRow" alignment="BASELINE_CENTER" prefWidth="300.0" spacing="19.0">
            <children>
                <Label fx:id="Username" text="Username" />
                <TextField fx:id="unameInput" alignment="CENTER" prefWidth="140.0" />
            </children>
         <VBox.margin>
            <Insets top="4.0" />
         </VBox.margin>
        </HBox>
        <HBox fx:id="passwordRow" alignment="BASELINE_CENTER" prefWidth="300.0" spacing="23.0">
            <children>
                <Label fx:id="Password" alignment="BOTTOM_LEFT" text="Password" />
                <TextField fx:id="passInput" alignment="CENTER" prefWidth="140.0" />
            </children>
         <VBox.margin>
            <Insets bottom="7.75" />
         </VBox.margin>
        </HBox>
    </children>
</VBox>
